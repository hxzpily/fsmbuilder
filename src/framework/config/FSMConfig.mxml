<?xml version="1.0" encoding="utf-8"?>
<swiz:Swiz xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:swiz="http://swiz.swizframework.org"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:config="framework.config.*" xmlns:beans="framework.config.beans.*">
	<swiz:config>
		<!-- The eventPackages value tells Swiz the path to your Event classes,
		and viewPackages is an optional value that speeds up the processing of display classes. -->
		<swiz:SwizConfig 
			eventPackages="framework.events.*" 
			viewPackages="mvcs.views.*" />
	</swiz:config>
	
	<!-- BeanProviders simply contain the non-display objects that Swiz should process. -->
	<swiz:beanProviders>
		<beans:ModelBeans/>
		<beans:ControllerBeans/>
		<beans:ServiceBeans/>
		<beans:ViewBeans/>
	</swiz:beanProviders>
</swiz:Swiz>
